    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/assert": "^1.5.6",
    "@types/emscripten": "^1.39.5",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.25",
    "@types/node-fetch": "^2.6.1",
    "@types/ws": "^8.5.3",
    "dts-bundle-generator": "^6.7.0",
    "error-stack-parser": "^2.0.6",
    "mocha": "^9.0.2",
    "prettier": "^2.2.1",
    "rollup": "^2.48.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.5",
