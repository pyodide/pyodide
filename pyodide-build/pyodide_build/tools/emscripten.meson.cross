# [binaries] section is ommited intentionally so that we can override the compiler using environment variables
[binaries]
exe_wrapper = 'node'

[properties]
needs_exe_wrapper = true
longdouble_format = 'IEEE_QUAD_LE' # for numpy

[host_machine]
system = 'emscripten'
cpu_family = 'wasm32'
cpu = 'wasm'
endian = 'little'