{
  "comment": "These are the default options except that we added pyodide-jsdoc-plugin.js",
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.ts(doc|x)?$",
    "excludePattern": "((^|\\/|\\\\)_)|(.*gen.*)"
  },
  "include": ["../**/*.ts"],
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
